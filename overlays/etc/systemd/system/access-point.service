[Unit]
Description=Start access point
Documentation=https://nims11.wordpress.com/2012/04/27/hostapd-the-linux-way-to-create-virtual-wifi-access-point/comment$
Wants=network-pre.target
After=network-pre.target NetworkManager.service systemd-resolved.service tailscaled.service

[Service]
ExecStart=/root/startHotspot.sh wlan0 tailscale0
Type=simple

[Install]
WantedBy=multi-user.target
